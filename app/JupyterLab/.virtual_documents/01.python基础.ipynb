class Parent:
    number = 100
    def __init__(self):
        print('调用父类构造函数')
    def parentM(self):
        print('调用父类的方法')
    def setAttr(self,attr):
        Parent.parentAttr=attr
    def getAttr(self):
        print('父类的属性：',Parent.parentAttr)
    def newM(self):
        print('父类要重新的方法')

class child(Parent):
    def __init__(self):
        print('调用子类的构造函数')
    def childM(self):
        print('调用子类的方法')
    def newM(self):
        print('子类覆盖的方法')


